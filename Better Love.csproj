        <Compile Include="Mian\CustomAssets\CultureTraits.cs" />
        <Compile Include="Mian\CustomAssets\DecisionAndBehaviorTasks.cs" />
        <Compile Include="Mian\CustomAssets\GodPowers.cs" />
        <Compile Include="Mian\CustomAssets\Happiness.cs" />
        <Compile Include="Mian\CustomAssets\QueerTraits.cs" />
        <Compile Include="Mian\CustomAssets\SubspeciesTraits.cs" />
        <Compile Include="Mian\TopicOfLoving.cs" />
